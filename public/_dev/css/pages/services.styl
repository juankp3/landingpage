section.sf-container-servicios
    background-color: #F5F5F5
    div.sf-servicios
        display: flex
        flex-direction: row
        justify-content: space-evenly;
        +media-s()
            display: flex
            flex-direction: column
            padding: 40px 0
        div.sf-data-servicios
            display: flex   
            flex-direction: column
            div.sf-especializados
                margin: 10% 0 0 0;
                +media-s()  
                    margin: 0 10%;
                div.sf-title-especializacion
                    color: $color-black
                    font-size: 20px
                    +media-s()
                        font-size: 28px
                    p
                        margin: 0 0 10px 0;
                div.sf-list-especializacion
                    ul.list-especializacion
                        list-style: none
                        margin: 0
                        padding: 0
                        li
                            background-image: url(../img/dot.png);
                            background-repeat: no-repeat;
                            background-position-y: center;
                            background-size: 6px;
                            padding: 4px 0 4px 12px;
                            font-family: UbuntuRegular;
                            font-size: 16px;
                            color: #00466f;
                            +media-s()
                                font-size: 18px;
            div.sf-complementarios
                margin: 10% 0 0 0;
                +media-s()
                    margin: 20px 10% 0 10%;
                div.sf-title-complementarios
                    color: $color-black
                    font-size: 20px
                    +media-s()
                        font-size: 28px
                    p
                        margin: 0 0 10px 0;
                div.sf-list-complementarios
                    ul.list-complementarios
                        list-style: none
                        margin: 0
                        padding: 0
                        li
                            background-image: url(../img/dot.png);
                            background-repeat: no-repeat;
                            background-position-y: center;
                            background-size: 6px;
                            padding: 4px 0 4px 12px;
                            font-family: UbuntuRegular;
                            font-size: 16px;
                            color: #00466f;
                            +media-s()
                                font-size: 18px;
        div.sf-img-servicios
            background-image: url(../img/bg_servicios.png);
            width: 792px;
            height 488px;
            +media-s()
                display: none

